		lineageTag: 420b0658-1afb-4756-9a5e-41f645fe50a5

	measure 'Revenue Contribution %' = DIVIDE([Revenue],CALCULATE([Revenue],ALL('sales products'),ALL('sales customers'),ALL('sales markets')))
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: c1a8df2f-42c6-47aa-bffc-d648ea3b1bf2

