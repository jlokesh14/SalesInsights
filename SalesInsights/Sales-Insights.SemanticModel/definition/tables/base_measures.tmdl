table base_measures
	lineageTag: da722513-8367-4084-86b1-b821b838f614

	measure Revenue = sum('sales transactions'[normalized_sales_amount])
		formatString: "₹"#,0.00;-"₹"#,0.00;"₹"#,0.00
		lineageTag: cbfbbcfb-b1e8-4d63-a7b4-9b6aee774ecb

		annotation PBI_FormatHint = {"currencyCulture":"hi-Latn-IN"}

	measure sales_Qty = sum('sales transactions'[sales_qty])
		formatString: 0
		lineageTag: b46e303c-b0ea-4977-838d-027bc25b6a7e

	measure 'Total profit margin' = sum('sales transactions'[profit_margin])
		lineageTag: e7815af7-4d92-46f1-adf7-b02fc79555a9

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'profit margin %' = DIVIDE([Total profit margin],[Revenue],0)
